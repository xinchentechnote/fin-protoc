    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package - format",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "format",
                "-f",
                "${env:HOME}/workspace/fin-proto-rs/szse-binary/proto/szse_bin_v1.29.dsl"
            ]
        },
        {
            "name": "Launch Package - compile",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "compile",
                "-f",
                "${env:HOME}/workspace/fin-proto/sample/sample.pdsl",
                "-j",
                "${env:HOME}/workspace/fin-proto-java/sample-bin/src/"
            ]
        },
        {
            "name": "Launch Package - debug gen lua",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "compile",
                "-f",
                "${env:HOME}/workspace/fin-proto/szse/binary/szse_bin_v1.29.pdsl",
                "-l",
                "${env:HOME}/workspace/fin-proto/szse/binary/"
            ]
        },
        {
            "name": "Launch Package - compile-for-go",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "compile",
                "-f",
                "${env:HOME}/workspace/fin-proto/sample/sample.pdsl",
                "-g",
                "${env:HOME}/workspace/fin-proto-go/sample-bin/messages/"
            ]
        },
        {
            "name": "Launch Package - compile sse",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "compile",
                "-f",
                "${env:HOME}/workspace/fin-proto-java/sse-bin/proto/sse_bin_v0.57.dsl",
                "-j",
                "${env:HOME}/workspace/fin-proto-java/sse-bin/src/"
            ]
        },
        {
            "name": "Launch Package - compile szse",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "compile",
                "-f",
                "${env:HOME}/workspace/fin-proto-java/szse-bin/proto/szse_bin_v1.29.pdsl",
                "-j",
                "${env:HOME}/workspace/fin-proto-java/szse-bin/src/"
            ]
        },
        {
            "name": "debug-gen-sample-rs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "compile",
                "-f",
                "${env:HOME}/workspace/fin-proto/sample/sample.pdsl",
                "-r",
                "${env:HOME}/workspace/fin-proto-rs/sample-binary/src/"
            ]
        }
    ]
}